{
  "name": "eslint-plugin-6fb",
  "version": "1.0.0",
  "description": "Custom ESLint rules for the 6fb-booking project to prevent duplicate components and bad patterns",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "6fb-booking"
  ],
  "author": "6FB Team",
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">=7.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@typescript-eslint/rule-tester": "^8.35.0",
    "@typescript-eslint/utils": "^6.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ]
  }
}
