{
  "name": "frontend-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "staging": "cp .env.staging .env.local && next dev -p 3001",
    "staging:turbo": "cp .env.staging .env.local && next dev --turbo -p 3001",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:profile": "next build --profile",
    "build:debug": "next build --debug",
    "build:production": "NODE_ENV=production next build",
    "build:cdn": "next build && npm run cdn:deploy",
    "cdn:validate": "cd .. && python scripts/cdn-deploy.py validate",
    "cdn:purge": "cd .. && python scripts/cdn-deploy.py purge",
    "cdn:status": "cd .. && python scripts/cdn-deploy.py status",
    "cdn:deploy": "cd .. && python scripts/cdn-deploy.py deploy --build-dir frontend-v2/.next",
    "start": "next start -p $PORT",
    "start:railway": "node .next/standalone/server.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:gdpr": "jest --testPathPattern=\"(CookieConsent|useCookieConsent|PrivacyDashboard)\"",
    "test:e2e": "playwright test",
    "test:e2e:gdpr": "playwright test tests/e2e/gdpr-compliance.spec.ts",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:calendar": "playwright test tests/e2e/calendar-views.spec.ts",
    "test:e2e:appointment": "playwright test tests/e2e/complete-appointment-flow.spec.ts",
    "test:calendar:full": "./scripts/run-calendar-test.sh",
    "test:puppeteer": "node tests/e2e/puppeteer/run-all-tests.js",
    "test:puppeteer:headed": "node tests/e2e/puppeteer/run-all-tests.js --headed",
    "test:puppeteer:quick": "node tests/e2e/puppeteer/run-all-tests.js --quick",
    "test:puppeteer:auth": "node tests/e2e/puppeteer/auth-flow-test.js",
    "test:puppeteer:registration": "node tests/e2e/puppeteer/registration-flow-test.js",
    "test:puppeteer:booking": "node tests/e2e/puppeteer/booking-flow-test.js",
    "test:puppeteer:dashboard": "node tests/e2e/puppeteer/dashboard-test.js",
    "test:puppeteer:mobile": "node tests/e2e/puppeteer/mobile-responsive-test.js",
    "test:puppeteer:performance": "node tests/e2e/puppeteer/performance-test.js",
    "test:puppeteer:errors": "node tests/e2e/puppeteer/error-handling-test.js",
    "test:puppeteer:scrolling": "node tests/e2e/puppeteer/registration-scrolling-test.js",
    "optimize": "npm run lint:fix && npm run build:analyze",
    "size": "npx size-limit",
    "lighthouse": "npx lighthouse http://localhost:3000 --output-path=./lighthouse-report.html",
    "lighthouse:ci": "npx lhci autorun",
    "lighthouse:calendar": "npx lighthouse http://localhost:3000/calendar --output-path=./lighthouse-calendar.html",
    "bundle:stats": "npx webpack-bundle-analyzer .next/analyze/client.json",
    "bundle:monitor": "node scripts/bundle-monitor.js",
    "bundle:check": "npm run build && npm run bundle:monitor",
    "performance:test": "node scripts/performance-test.js",
    "performance:quick": "npm run build && npm run lighthouse:calendar",
    "accessibility:test": "node scripts/accessibility-test.js",
    "accessibility:audit": "npx pa11y http://localhost:3000/calendar --reporter html > accessibility-audit.html",
    "cross-browser:test": "node scripts/cross-browser-test.js",
    "integration:test": "node scripts/integration-test.js",
    "test:comprehensive": "node scripts/test-orchestrator.js",
    "test:validate": "npm run performance:test && npm run accessibility:test && npm run cross-browser:test && npm run integration:test",
    "precommit": "npm run lint && npm run test",
    "env": "node scripts/show-environment.js",
    "env:check": "node scripts/check-ports.js",
    "ports": "lsof -i :3000 && lsof -i :3001 && lsof -i :8000 && lsof -i :8001 || echo 'Checking ports...'",
    "kill:staging": "lsof -ti:3001 | xargs kill -9 && lsof -ti:8001 | xargs kill -9 || echo 'No staging processes to kill'"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-spring/web": "^10.0.1",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.4.0",
    "@tanstack/react-query": "^5.83.0",
    "@types/jspdf": "^1.3.3",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "framer-motion": "^12.23.9",
    "glob": "^11.0.3",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.525.0",
    "next": "^14.2.30",
    "postcss": "^8.4.39",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-day-picker": "^9.8.0",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "recharts": "^3.1.0",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "web-vitals": "^4.2.4",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@lhci/cli": "^0.13.0",
    "@next/bundle-analyzer": "^15.3.4",
    "@playwright/test": "^1.53.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.4",
    "axe-core": "^4.8.3",
    "axe-playwright": "^2.0.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.30",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-watch-typeahead": "^2.2.2",
    "jsdom": "^26.1.0",
    "lighthouse": "^11.0.0",
    "msw": "^2.10.2",
    "next-router-mock": "^0.9.13",
    "node-fetch": "^2.7.0",
    "pa11y": "^8.0.0",
    "puppeteer": "^24.11.2",
    "ts-node": "^10.9.2",
    "whatwg-fetch": "^3.6.20"
  }
}
