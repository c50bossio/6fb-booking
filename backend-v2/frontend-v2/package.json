{
  "name": "frontend-v2",
  "version": "0.1.0",
  "private": true,
  "//": "Cache bypass 2025-07-15-v2 - Force rebuild after recurring cache corruption",
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:profile": "next build --profile",
    "build:debug": "next build --debug",
    "build:production": "NODE_ENV=production next build",
    "start": "next start -p $PORT",
    "start:railway": "node .next/standalone/server.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:gdpr": "jest --testPathPattern=\"(CookieConsent|useCookieConsent|PrivacyDashboard)\"",
    "test:e2e": "playwright test",
    "test:e2e:gdpr": "playwright test tests/e2e/gdpr-compliance.spec.ts",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:calendar": "playwright test tests/e2e/calendar-views.spec.ts",
    "test:e2e:appointment": "playwright test tests/e2e/complete-appointment-flow.spec.ts",
    "test:calendar:full": "./scripts/run-calendar-test.sh",
    "test:puppeteer": "node tests/e2e/puppeteer/run-all-tests.js",
    "test:puppeteer:headed": "node tests/e2e/puppeteer/run-all-tests.js --headed",
    "test:puppeteer:quick": "node tests/e2e/puppeteer/run-all-tests.js --quick",
    "test:puppeteer:auth": "node tests/e2e/puppeteer/auth-flow-test.js",
    "test:puppeteer:registration": "node tests/e2e/puppeteer/registration-flow-test.js",
    "test:puppeteer:booking": "node tests/e2e/puppeteer/booking-flow-test.js",
    "test:puppeteer:dashboard": "node tests/e2e/puppeteer/dashboard-test.js",
    "test:puppeteer:mobile": "node tests/e2e/puppeteer/mobile-responsive-test.js",
    "test:puppeteer:performance": "node tests/e2e/puppeteer/performance-test.js",
    "test:puppeteer:errors": "node tests/e2e/puppeteer/error-handling-test.js",
    "context7:start": "npx @upstash/context7-mcp",
    "context7:test": "npx @modelcontextprotocol/inspector npx @upstash/context7-mcp",
    "context7:http": "npx @upstash/context7-mcp --transport http --port 3007",
    "docs:react": "echo 'Use: mcp__context7__get-library-docs context7CompatibleLibraryID=/context7/react_dev topic=hooks tokens=2000'",
    "docs:nextjs": "echo 'Use: mcp__context7__get-library-docs context7CompatibleLibraryID=/context7/nextjs topic=\"app router\" tokens=2000'",
    "docs:fastapi": "echo 'Use: mcp__context7__get-library-docs context7CompatibleLibraryID=/tiangolo/fastapi topic=\"dependency injection\" tokens=2000'",
    "docs:stripe": "echo 'Use: mcp__context7__get-library-docs context7CompatibleLibraryID=/stripe/stripe-python topic=\"payment intents\" tokens=2000'",
    "docs:sqlalchemy": "echo 'Use: mcp__context7__get-library-docs context7CompatibleLibraryID=/sqlalchemy/sqlalchemy topic=relationships tokens=2500'",
    "docs:search": "echo 'Use: mcp__context7__resolve-library-id libraryName=\"library-name\"'",
    "test:puppeteer:scrolling": "node tests/e2e/puppeteer/registration-scrolling-test.js",
    "optimize": "npm run lint:fix && npm run build:analyze",
    "size": "npx size-limit",
    "lighthouse": "npx lighthouse http://localhost:3000 --output-path=./lighthouse-report.html",
    "bundle:stats": "npx webpack-bundle-analyzer .next/analyze/client.json",
    "precommit": "npm run lint && npm run test",
    "env": "node scripts/show-environment.js",
    "env:check": "node scripts/check-ports.js",
    "ports": "lsof -i :3000 && lsof -i :8000 || echo 'Checking ports...'"
  },
  "dependencies": {
    "@builder.io/react": "^8.2.5",
    "@builder.io/sdk": "^6.1.1",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-spring/web": "^10.0.1",
    "@sentry/nextjs": "^8.55.0",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.4.0",
    "@tanstack/react-query": "^5.81.5",
    "@types/qrcode": "^1.5.5",
    "@upstash/context7-mcp": "github:upstash/context7",
    "autoprefixer": "^10.4.19",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "glob": "^11.0.3",
    "lucide-react": "^0.525.0",
    "next": "^14.2.30",
    "postcss": "^8.4.39",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.4",
    "web-vitals": "^4.2.4",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.3.4",
    "@playwright/test": "^1.53.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint": "^9.30.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-watch-typeahead": "^2.2.2",
    "jsdom": "^26.1.0",
    "msw": "^2.10.2",
    "next-router-mock": "^0.9.13",
    "node-fetch": "^2.7.0",
    "puppeteer": "^24.11.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "whatwg-fetch": "^3.6.20"
  }
}
