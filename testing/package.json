{
  "name": "bookedbarber-load-testing",
  "version": "1.0.0",
  "description": "Comprehensive load testing suite for BookedBarber V2 - validating 10,000+ concurrent user capacity",
  "scripts": {
    "install": "npm install",
    "test:load": "artillery run artillery-configs/load-test-comprehensive.yml",
    "test:smoke": "artillery run artillery-configs/smoke-test.yml",
    "test:stress": "artillery run artillery-configs/stress-test.yml",
    "test:spike": "artillery run artillery-configs/spike-test.yml",
    "test:endurance": "artillery run artillery-configs/endurance-test.yml",
    "test:api": "artillery run artillery-configs/api-endpoints.yml",
    "test:booking": "artillery run artillery-configs/booking-flow.yml",
    "test:auth": "artillery run artillery-configs/auth-flow.yml",
    "test:payments": "artillery run artillery-configs/payment-flow.yml",
    "test:calendar": "artillery run artillery-configs/calendar-sync.yml",
    "test:database": "node scripts/database-load-test.js",
    "test:gradual": "node scripts/gradual-load-test.js",
    "test:performance": "node scripts/performance-benchmarks.js",
    "test:regression": "node scripts/regression-test.js",
    "test:all": "node scripts/run-all-tests.js",
    "monitor:start": "node scripts/monitoring-collector.js",
    "monitor:dashboard": "node scripts/performance-dashboard.js",
    "report:generate": "node scripts/generate-reports.js",
    "report:html": "node scripts/html-report-generator.js",
    "analyze:results": "node scripts/analyze-test-results.js",
    "setup:test-data": "node scripts/setup-test-data.js",
    "cleanup:test-data": "node scripts/cleanup-test-data.js",
    "validate:system": "node scripts/system-validation.js"
  },
  "dependencies": {
    "artillery": "^2.0.0",
    "artillery-plugin-metrics-by-endpoint": "^1.2.0",
    "artillery-plugin-statsd": "^3.0.0",
    "artillery-plugin-publish-metrics": "^3.1.0",
    "artillery-plugin-expect": "^1.2.0",
    "artillery-plugin-hls": "^2.0.0",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "cli-table3": "^0.6.3",
    "node-cron": "^3.0.2",
    "ws": "^8.13.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.0",
    "mysql2": "^3.3.3",
    "sqlite3": "^5.1.6",
    "csv-parse": "^5.4.0",
    "csv-stringify": "^6.4.0",
    "html-pdf": "^3.0.1",
    "puppeteer": "^20.5.0",
    "chart.js": "^4.3.0",
    "chartjs-node-canvas": "^4.1.6",
    "prometheus-api-metrics": "^3.2.2",
    "influxdb-client": "^1.33.2",
    "statsd-client": "^0.4.7"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "typescript": "^5.1.3",
    "nodemon": "^2.0.22",
    "concurrently": "^8.2.0"
  },
  "keywords": [
    "load-testing",
    "performance",
    "artillery",
    "bookedbarber",
    "fastapi",
    "nextjs",
    "scalability"
  ],
  "author": "BookedBarber Team",
  "license": "MIT"
}